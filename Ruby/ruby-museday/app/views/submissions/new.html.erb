<h1>New Submission</h1>

<%= form_for :submission, :url => submissions_path do |f| %>
   <p>
     <%= f.label :date %><br />
     <%= f.date_select :date, :start_year => 2010 %>
   </p>

      <p>
     <%= f.label :followup_date %><br />
     <%= f.date_select :followup_date, :start_year => 2013 %>
   </p>

   <p>
      <%= f.label :notes %><br />
      <%= f.text_area :notes, :id => "notes_area" %>
   </p>

   <p>
   <%= f.submit %>
   </p>
<% end %>
